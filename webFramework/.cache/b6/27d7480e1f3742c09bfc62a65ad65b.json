ource":"src/views/UserForm.ts","name":null,"original":{"line":71,"column":16},"generated":{"line":97,"column":18}},{"source":"src/views/UserForm.ts","name":null,"original":{"line":71,"column":4},"generated":{"line":97,"column":24}},{"source":"src/views/UserForm.ts","name":null,"original":{"line":71,"column":23},"generated":{"line":97,"column":25}},{"source":"src/views/UserForm.ts","name":null,"original":{"line":71,"column":38},"generated":{"line":97,"column":40}},{"source":"src/views/UserForm.ts","name":null,"original":{"line":71,"column":39},"generated":{"line":97,"column":41}},{"source":"src/views/UserForm.ts","name":null,"original":{"line":71,"column":4},"generated":{"line":97,"column":48}},{"source":"src/views/UserForm.ts","name":null,"original":{"line":72,"column":3},"generated":{"line":98,"column":0}},{"source":"src/views/UserForm.ts","name":null,"original":{"line":5,"column":0},"generated":{"line":104,"column":0}},{"source":"src/views/UserForm.ts","name":null,"original":{"line":5,"column":0},"generated":{"line":104,"column":7}},{"source":"src/views/UserForm.ts","name":null,"original":{"line":5,"column":0},"generated":{"line":104,"column":8}},{"source":"src/views/UserForm.ts","name":null,"original":{"line":5,"column":0},"generated":{"line":104,"column":16}},{"source":"src/views/UserForm.ts","name":null,"original":{"line":5,"column":0},"generated":{"line":104,"column":19}},{"source":"src/views/UserForm.ts","name":null,"original":{"line":5,"column":0},"generated":{"line":104,"column":27}}],"sources":{"src/views/UserForm.ts":"import { User } from \"../models/User\";\n\ntype Function = () => void\n\nexport class UserForm {\n  constructor ( public parent: Element, public model: User ) {\n    this.bindModel();\n  }\n\n  bindModel = (): void => {\n    this.model.on('change', () => {\n      this.render();\n    });\n  }    \n\n  eventsMap = (): { [key: string]: Function} => {\n    return {\n      'click:button': this.onButtonClick,\n      'mouseenter:h1': this.onHeaderHover,\n      'click:.set-age': this.onSetAgeClick\n    };\n  }\n\n  onButtonClick = (): void => {\n    console.log('Hi there!');\n  }\n\n  onHeaderHover = (): void => {\n    console.log('Hovering!');\n  }\n\n  onSetAgeClick = (): void => {\n    this.model.setRandomAge();\n  }\n\n  bindEvents = (fragment: DocumentFragment): void => {\n    const eventsMap = this.eventsMap();\n\n    for (let eventKey in eventsMap) {\n      // Destructuring array:\n      // 'click:button' --> [click, button]\n      const [eventName, selector] = eventKey.split(':');\n\n      fragment.querySelectorAll(selector).forEach( element => {\n        element.addEventListener(eventName, eventsMap[eventKey]);\n      });\n    }\n  }\n\n  template(): string {\n    return `\n      <div> \n        <h1>User form</h1>\n        <div> User name: ${this.model.get('name')} </div>\n        <div> User age: ${this.model.get('age')} </div>\n        <input />\n        <button> Click-me! </button>\n        <button class=\"set-age\"> Set Random Age </button>\n      </>\n    `\n  }\n\n  render(): void {\n    this.parent.innerHTML = '';\n    \n    const templateElement = document.createElement('template');\n    templateElement.innerHTML = this.template();\n\n    this.bindEvents(templateElement.content);\n\n    this.parent.append(templateElement.content);\n  }\n}"},"lineCount":null}},"error":null,"hash":"4e7e029b0e88ace245d6d6830e286978","cacheData":{"env":{}}}